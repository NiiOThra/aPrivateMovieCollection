<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="142.0" prefWidth="388.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.deleteMovieView.DeleteMovieController">
   <children>
      <Label layoutX="20.0" layoutY="14.0" text="Are you sure you want to delete this movie?">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button fx:id="btnYes" layoutX="59.0" layoutY="62.0" mnemonicParsing="false" onAction="#doDelete" prefHeight="50.0" prefWidth="90.0" text="Yes" />
      <Button fx:id="btnNo" layoutX="240.0" layoutY="62.0" mnemonicParsing="false" onAction="#close" prefHeight="50.0" prefWidth="90.0" text="No" />
   </children>
</AnchorPane>
